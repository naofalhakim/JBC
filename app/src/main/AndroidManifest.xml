<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="id.co.venport.vendorportalandroidapplication">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_portalvendor"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".SplashLoginRegister.LoadingActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SplashLoginRegister.TampilanAwalActivity" />
        <activity android:name=".SplashLoginRegister.LoginActivity" />
        <activity android:name=".SplashLoginRegister.lupapasswordActivity" />
        <activity android:name=".SplashLoginRegister.RegisterActivity" />
        <activity android:name=".HomePageActivity" />
        <activity android:name=".SplashLoginRegister.SliderActivity" />
        <activity
            android:name=".mainmenu.MenuActivity"
            android:theme="@style/AppTheme"></activity>
    </application>

</manifest>